<p id="notice"><%= notice %></p>

<h1>Document State Rels</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Name</th>
      <th>Description</th>
      <th>Obs</th>
      <th>Document</th>
      <th>Document state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @document_state_rels.each do |document_state_rel| %>
      <tr>
        <td><%= document_state_rel.date %></td>
        <td><%= document_state_rel.name %></td>
        <td><%= document_state_rel.description %></td>
        <td><%= document_state_rel.obs %></td>
        <td><%= document_state_rel.document_id %></td>
        <td><%= document_state_rel.document_state_id %></td>
        <td><%= link_to 'Show', document_state_rel %></td>
        <td><%= link_to 'Edit', edit_document_state_rel_path(document_state_rel) %></td>
        <td><%= link_to 'Destroy', document_state_rel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Document State Rel', new_document_state_rel_path %>
