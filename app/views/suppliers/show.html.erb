<div class="content content-fixed">
      <div class="container pd-x-0 pd-lg-x-10 pd-xl-x-0">
        <div class="d-sm-flex align-items-center justify-content-between mg-b-20 mg-lg-b-30">
          <div>
            <nav aria-label="breadcrumb">
              <ol class="breadcrumb breadcrumb-style1 mg-b-10">
                <li class="breadcrumb-item"><a href="#">Proveedores</a></li>
                <li class="breadcrumb-item active" aria-current="page">Detalle</li>
              </ol>
            </nav>
          </div>
        </div>
        <div class="row row-xs">
          <div class="col-lg-12">
            <div class="card">
              <div class="card-header d-sm-flex justify-content-between bd-b-0 pd-t-20 pd-b-0">
              </div>
              <div class="card-body">
              <h2>Proveedor: <%= @supplier.name%></h2>
              <p>
    <strong>Nombre:</strong>
    <%= @supplier.name%>
</p>
<p>
    <strong>Doc. Identidad:</strong>
    <%= @supplier.doc_ident%>
</p>
<p>
    <strong>Dirección:</strong>
    <%= @supplier.street%>
</p>
<p>
    <strong>Email:</strong>
    <%= @supplier.email%>
</p>
<p>
    <strong>Celular:</strong>
    <%= @supplier.mobile%>
</p>



<table id="example1" class="table table-striped table-sm">
<colgroup>
<col width="20%"/>
<col width="20%"/>
<col width="20%"/>
<col width="20%"/>
<col width="20%"/>
</colgroup>
<thead>
<tr>
<th scope="col">Código</th>
<th scope="col">Nro. Documento</th>
<th scope="col">Monto Total</th>
<th scope="col">Estado</th>
<th></th>
</tr>
</thead>
<tbody>
<% @documents.each do |d| %>
<tr>
<td class="align-middle"><%= d.code %></td>
<td class="align-middle"><%= d.doc_code %></td>
<td class="align-middle"><%= d.total_amount %></td>
<td class="align-middle"><%= d.state %></td>
<td class="align-middle"><%= link_to '<i class="fas fa-eye"></i>'.html_safe, { :action => :show, :id => d.id }, :class=>"btn btn-xs btn-primary btn-icon" %>
<%= link_to '<i class="fas fa-pen"></i>'.html_safe, { :action => :edit, :id => d.id }, :class=>"btn btn-xs btn-success btn-icon" %>
<%= link_to '<i class="fas fa-trash"></i>'.html_safe, { :action => :destroy, :id => d.id }, :class=>"btn btn-xs btn-danger btn-icon", :method => :delete, :data => { :confirm => 'Estas Seguro?' } %></td>
</tr>
<% end %>
</tbody>
</table>
              </div><!-- card-body -->
              <div class="card-footer pd-y-15 pd-x-20">
              <%= link_to 'Volver', { :action => :index }, :class => "btn btn-primary" %> 
              <%= link_to 'Editar', {:action => :edit, :id => @supplier.id }, :class => "btn btn-success" %>
              </div><!-- card-footer -->
            </div><!-- card -->
          </div><!-- col -->
        </div><!-- row -->
      </div><!-- container -->
    </div><!-- content -->